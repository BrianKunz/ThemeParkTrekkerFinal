{
  "version": "1.0.0",
  "name": "basic-ts-boiler",
  "keywords": [
    "boilderplate"
  ],
  "author": "me@dominiccobb.dev Dominic Cobb",
  "license": "MIT",
  "description": "",
  "main": "./src/main.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/main.ts",
    "postinstall": "tsc",
    "watch-node": "nodemon dist/main.ts",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-node && yarn esbuild-browser",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "chalk": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "pg": "^8.10.0",
    "ph": "^0.1.10",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "typeorm": "^0.3.15",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^17.0.35",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.2.0",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "esbuild": "^0.14.41",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typedoc": "^0.22.15"
  }
}
